{
  "npmScope": "webneek",
  "affected": {
    "defaultBase": "main"
  },
  "implicitDependencies": {
    "workspace.json": "*",
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    "tsconfig.base.json": "*",
    "tslint.json": "*",
    ".eslintrc.json": "*",
    "nx.json": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": {
        "cacheableOperations": ["build", "lint", "test", "e2e"]
      }
    }
  },
  "projects": {
    "server": {
      "tags": ["side:server"]
    },
    "client": {
      "tags": ["side:client"]
    },
    "client-e2e": {
      "tags": [],
      "implicitDependencies": ["client"]
    },
    "media": {
      "tags": []
    },
    "rtc-data": {
      "tags": []
    },
    "server-core": {
      "tags": ["side:server"]
    }
  },
  "workspaceLayout": {
    "appsDir": "samples",
    "libsDir": "packages"
  }
}
